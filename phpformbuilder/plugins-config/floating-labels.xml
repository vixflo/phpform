<?xml version="1.0" encoding="UTF-8"?>
<root>
    <default>
        <includes>
            <!--
                Path to css and js files into plugins folder.
                If your files are not in plugins folder, use absolute paths.
            -->
            <css></css>
            <js>
                <file>floating-labels/ShadowFloatingLabels.min.js</file>
            </js>
        </includes>
        <js_code><![CDATA[    if (typeof(phpfbFloatingLabels) == "undefined") {
        window.phpfbFloatingLabels = [];
    }
    phpfbFloatingLabels["%selector%"] = new ShadowFloatingLabels("%selector%");]]>
        </js_code>
    </default>
    <custom>
        <!--
            If you need differents includes for your custom code,
            copy the default '<includes>' structure and put the needed files into '<file>' nodes.
            You can add several files nodes if necessary.
        -->
        <js_code>
            <!-- your custom code here -->
        </js_code>
    </custom>
</root>
